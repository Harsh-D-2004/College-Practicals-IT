package com.exphandling.main;

import java.util.Scanner;

public class Main {
	public static void main(String Args[])
	{
		Scanner sobj = new Scanner(System.in);
		
		int num1 = 0;
		int num2 = 0;
		
		System.out.println("Enter Number 1 : ");
		num1 = sobj.nextInt();
		
		System.out.println("Enter Number 2 : ");
		num2 = sobj.nextInt();
	}
}
